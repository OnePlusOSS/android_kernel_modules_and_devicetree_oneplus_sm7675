&pm8550vs_c_gpios {
  //#ifdef OPLUS_FEATRUE_DUAL_SIM_DETECT
  sim2_det_no_pull: sim2_det_no_pull {
    pins = "gpio5";
    function = "normal";
    input-enable;
    power-source = <1>;
    bias-disable;
  };

  sim2_det_pull_high: sim2_det_pull_high {
    pins = "gpio5";
    function = "normal";
    input-enable;
    power-source = <1>;
    bias-pull-up;
  };

  sim2_det_pull_low: sim2_det_pull_low {
    pins = "gpio5";
    function = "normal";
    input-enable;
    power-source = <1>;
    bias-pull-down;
