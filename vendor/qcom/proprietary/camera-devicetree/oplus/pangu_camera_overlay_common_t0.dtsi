&cam_csiphy0{
	cell-index = <0>;
	compatible = "qcom,csiphy-v2.2.0-enhance1", "qcom,csiphy";
};

&cam_csiphy1{
        cell-index = <1>;
        compatible = "qcom,csiphy-v2.2.0-enhance1", "qcom,csiphy";
};

&cam_csiphy2{
	cell-index = <2>;
	compatible = "qcom,csiphy-v2.2.0-enhance1", "qcom,csiphy";
};

&cam_csiphy5{
        cell-index = <5>;
        compatible = "qcom,csiphy-v2.2.0-enhance1", "qcom,csiphy";
